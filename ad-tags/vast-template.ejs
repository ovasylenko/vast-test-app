<?xml version="1.0" encoding="UTF-8"?>
<VAST version="2.0">
    <Ad id="preroll-1">
        <InLine>
            <AdSystem>Mone VAST</AdSystem>
            <AdTitle><%= campaignId %></AdTitle>
            <Creatives>
                <Creative>
                    <Linear skipoffset="30%">
                        <Impression>
                            <![CDATA[<%= trackUrl %>impression?userId=<%= userId %>]]>
                        </Impression>
                        <TrackingEvents>
                            <Tracking event="creativeView"><![CDATA[<%= trackUrl %>creativeView?userId=<%= userId %>]]></Tracking>
                            <Tracking event="start"><![CDATA[<%= trackUrl %>start?userId=<%= userId %>]]></Tracking>
                            <Tracking event="midpoint"><![CDATA[<%= trackUrl %>midpoint?userId=<%= userId %>]]></Tracking>
                            <Tracking event="firstQuartile"><![CDATA[<%= trackUrl %>firstQuartile?userId=<%= userId %>]]></Tracking>
                            <Tracking event="thirdQuartile"><![CDATA[<%= trackUrl %>thirdQuartile?userId=<%= userId %>]]></Tracking>
                            <Tracking event="complete"><![CDATA[<%= trackUrl %>complete?userId=<%= userId %>]]></Tracking>
                        </TrackingEvents>
                        <VideoClicks>
                            <ClickThrough id="clickme">
                                <![CDATA[ <%= redirectUrl %> ]]>
                            </ClickThrough>
                            <ClickTracking><![CDATA[<%= trackUrl %>click]]></ClickTracking>
                        </VideoClicks>
                        <MediaFiles>
                            <MediaFile height="<%= height %>" width="<%= width %>" type="<%= mimeType %>">
                                <![CDATA[<%= mediaFileUrl %>]]>
                            </MediaFile>
                        </MediaFiles>
                    </Linear>
                </Creative>
            </Creatives>
        </InLine>
    </Ad>
</VAST>